{
  "name": "authzed",
  "publisher": "chiperific",
  "displayName": "authzed-language-support",
  "description": "Authzed language support for .zed files",
  "version": "1.1.1",
  "engines": {
    "vscode": "^1.76.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/chiperific/vscode_authzed_syntax.git"
  },
  "homepage": "https://github.com/chiperific/vscode_authzed_syntax",
  "categories": [
    "Programming Languages"
  ],
  "keywords": [
    "authzed",
    "Authzed",
    "schema.zed",
    "SpiceDb"
  ],
  "contributes": {
    "languages": [
      {
        "id": "authzed",
        "aliases": [
          "Authzed",
          "authzed"
        ],
        "extensions": [
          ".zed"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "authzed",
        "scopeName": "source.authzed",
        "path": "./syntaxes/authzed.tmGrammar.json"
      }
    ]
  },
  "dependencies": {
    "plist2": "^1.1.3"
  }
}
